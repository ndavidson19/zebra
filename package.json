{
    "name": "apic-bootstrap",
    "version": "0.1.0",
    "private": true,
    "proxy": "http://localhost:5000",
    "dependencies": {
        "@bui/magnetic": "^7.10.24",
        "@bui/material": "^1.0.1",
        "@bui/theming": "^1.0.31",
        "@mui/material": "^5.8.6",
        "apic-api-infra": "1.1.1",
        "axios": "^0.27.2",
        "bcrypt": "^5.0.1",
        "blueprint-react": "3.9.17",
        "body-parser": "^1.20.0",
        "browserslist": "4.20.0",
        "dotenv": "^16.0.1",
        "ejs": "^3.1.8",
        "express": "^4.18.1",
        "express-flash": "0.0.2",
        "express-session": "^1.17.3",
        "http-proxy-middleware": "2.0.3",
        "husky": "7.0.4",
        "jsonwebtoken": "^8.5.1",
        "jwt-decode": "^3.1.2",
        "lint-staged": "12.3.5",
        "lodash": "4.17.21",
        "method-override": "^3.0.0",
        "nodemon": "^2.0.18",
        "passport": "^0.6.0",
        "passport-local": "^1.0.0",
        "prettier": "2.5.1",
        "prop-types": "15.8.1",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-error-overlay": "6.0.9",
        "react-grid-layout": "1.3.4",
        "react-redux": "^7.2.6",
        "react-router-dom": "^5.1.0",
        "react-router-redux": "4.0.8",
        "react-scripts": "4.0.3",
        "react-validation": "^3.0.7",
        "redux": "^4.1.2",
        "redux-promise-middleware": "5.1.1",
        "redux-thunk": "^2.4.1",
        "sass": "1.49.9",
        "universal-cookie": "4.0.4",
        "validator": "^13.7.0"
    },
    "devDependencies": {
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.25.4",
        "jscpd": "3.4.5"
    },
    "homepage": ".",
    "scripts": {
        "client": "cd client && npm start",
        "server": "nodemon index.js",
        "dev": "concurrently --kill-others-on-fail \"npm server\" \"npm client\"",
        "start": "DISABLE_ESLINT_PLUGIN='true' react-scripts start",
        "start-secure": "DISABLE_ESLINT_PLUGIN='true' HTTPS=true PORT=3443 react-scripts start",
        "build": "DISABLE_ESLINT_PLUGIN='true' react-scripts build",
        "install-hooks": "husky install .git/hooks && husky set .git/hooks/pre-commit \"npm run pre-commit\"",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "react-scripts build",
        "pre-commit": "lint-staged",
        "lint": "eslint --ignore-path .gitignore ."
    },
    "lint-staged": {
        "src/**/*.{js,jsx}": [
            "eslint --max-warnings=0",
            "prettier --write",
            "jscpd --reporters consoleFull -t 10"
        ],
        "src/**/*.{json,css,scss,md}": [
            "prettier --write"
        ]
    },
    "prettier": {
        "printWidth": 120,
        "bracketSpacing": false,
        "tabWidth": 4,
        "singleQuote": true,
        "jsxBracketSameLine": true,
        "jsxSingleQuote": true
    },
    "eslintConfig": {
        "settings": {
            "import/resolver": {
                "node": {
                    "extensions": [
                        ".js",
                        ".jsx"
                    ],
                    "moduleDirectory": [
                        "src",
                        "node_modules"
                    ]
                }
            }
        },
        "extends": [
            "react-app",
            "airbnb",
            "prettier"
        ],
        "rules": {
            "no-underscore-dangle": "off",
            "import/no-cycle": "off",
            "import/prefer-default-export": "off",
            "no-class-assign": "off",
            "no-plusplus": "off",
            "jsx-a11y/anchor-is-valid": "off",
            "jsx-a11y/no-static-element-interactions": "off",
            "jsx-a11y/click-events-have-key-events": "off",
            "react/jsx-props-no-spreading": "off"
        }
    },
    "eslintIgnore": [],
    "browserslist": {
        "production": [
            ">0.2%",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
